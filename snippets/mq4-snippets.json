    "enumTradesMT4": {
        "body": 
        [
            "int total = OrdersTotal();",
            "for (int ${1:i} = 0; ${1:i} < total; ${1:i}++)",
            "{",
            "   if (!OrderSelect(${1:i}, SELECT_BY_POS, MODE_TRADES))",
            "   {",
            "      continue;",
            "   }",
            "   int orderType = OrderType();",
            "   if (orderType != OP_BUY && orderType != OP_SELL)",
            "   {",
            "      continue;",
            "   }",
            "}"
        ],
        "description": "MT4: Enum trades",
        "prefix": "enumTradesMT4",
        "scope": "source.mq4"
    },
